{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,  // TODO: strategy 에러 해결 후 true로 전호나
    "noUnusedParameters": false,  // TODO: strategy 에러 해결 후 true로 전호나
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "strictPropertyInitialization": false
  }
}
